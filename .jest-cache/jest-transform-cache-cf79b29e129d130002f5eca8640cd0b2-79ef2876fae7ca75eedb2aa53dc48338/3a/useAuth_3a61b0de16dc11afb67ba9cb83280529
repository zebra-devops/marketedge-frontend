        /* istanbul ignore next */
        {
          cov_220e8v818s().b[1][1]++;
        }
      } catch (error) {
        /* istanbul ignore next */
        cov_220e8v818s().s[24]++;
        console.error("Auth initialization failed:", error);
        /* istanbul ignore next */
        cov_220e8v818s().s[25]++;
        _auth.authService.logout();
      } finally {
        /* istanbul ignore next */
        cov_220e8v818s().s[26]++;
        setIsLoading(false);
      }
    };
    /* istanbul ignore next */
    cov_220e8v818s().s[27]++;
    initAuth();
  }, []);
  /* istanbul ignore next */
  cov_220e8v818s().s[28]++;
  const login = async (code, redirectUri) => {
    /* istanbul ignore next */
    cov_220e8v818s().f[8]++;
    cov_220e8v818s().s[29]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_220e8v818s().s[30]++, await _auth.authService.login({
        code,
        redirect_uri: redirectUri
      }));
      /* istanbul ignore next */
      cov_220e8v818s().s[31]++;
      setUser(response.user);
    } catch (error) {
      /* istanbul ignore next */
      cov_220e8v818s().s[32]++;
      console.error("Login failed:", error);
      /* istanbul ignore next */
      cov_220e8v818s().s[33]++;
      throw error;
    }
  };
  /* istanbul ignore next */
  cov_220e8v818s().s[34]++;
  const logout = () => {
    /* istanbul ignore next */
    cov_220e8v818s().f[9]++;
    cov_220e8v818s().s[35]++;
    _auth.authService.logout();
    /* istanbul ignore next */
    cov_220e8v818s().s[36]++;
    setUser(null);
  };
  /* istanbul ignore next */
  cov_220e8v818s().s[37]++;
  const refreshUser = async () => {
    /* istanbul ignore next */
    cov_220e8v818s().f[10]++;
    cov_220e8v818s().s[38]++;
    try {
      const userData =
      /* istanbul ignore next */
      (cov_220e8v818s().s[39]++, await _auth.authService.getCurrentUser());
      /* istanbul ignore next */
      cov_220e8v818s().s[40]++;
      setUser(userData);
    } catch (error) {
      /* istanbul ignore next */
      cov_220e8v818s().s[41]++;
      console.error("User refresh failed:", error);
      /* istanbul ignore next */
      cov_220e8v818s().s[42]++;
      logout();
    }
  };
  /* istanbul ignore next */
  cov_220e8v818s().s[43]++;
  return {
    user,
    isAuthenticated: !!user,
    isLoading,
    login,
    logout,
    refreshUser
  };
};
